{
    "name": "multi-tenant-saas-backend",
    "version": "1.0.0",
    "main": "dist/src/server.js",
    "type": "module",
    "scripts": {
        "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
        "build": "tsc -p .",
        "start": "NODE_ENV=production node dist/src/server.js",
        "migrate": "npm run build && node dist/scripts/migrate.js",
        "seed": "npm run build && node dist/scripts/seed.js",
        "init": "npm run build && node dist/scripts/init.js"
    },
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-validator": "^7.0.1",
        "helmet": "^7.0.0",
        "jsonwebtoken": "^9.0.2",
        "morgan": "^1.10.0",
        "pg": "^8.11.3",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.6",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/express-validator": "^3.0.0",
        "@types/joi": "^17.2.2",
        "@types/jsonwebtoken": "^9.0.6",
        "@types/morgan": "^1.9.7",
        "@types/node": "^20.14.12",
        "@types/uuid": "^9.0.8",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.4.5"
    }
}